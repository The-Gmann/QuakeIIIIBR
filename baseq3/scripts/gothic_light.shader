textures/gothic_light/crosslt1
{
	light 1
	surfacelight 3250
	surfaceparm nomarks
	{
		map $lightmap
	}
	{
		map textures/gothic_light/crosslt1.tga
		blendFunc GL_DST_COLOR GL_ZERO
	}
	{
		map textures/gothic_light/crosslt1.blend.tga
		rgbGen wave sin 0.5 0.5 0 .2
		blendFunc GL_ONE GL_ONE
	}
}
